<Window x:Class="Project_1___Pokemon_WPF.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_1___Pokemon_WPF"
        mc:Ignorable="d"
        Title="Register" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="Button" Margin="100,195,117,0" VerticalAlignment="Top" Click="button_Click"/>
        <TextBox x:Name="txt_username" HorizontalAlignment="Left" Height="23" Margin="75,50,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txt_password" HorizontalAlignment="Left" Height="23" Margin="75,100,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txt_mail" HorizontalAlignment="Left" Height="23" Margin="75,155,0,0" TextWrapping="Wrap" Text="mail" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
